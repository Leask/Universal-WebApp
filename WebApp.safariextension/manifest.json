{
    "name": "__MSG_appName__",
    "version": "2.1.3",
    "description": "__MSG_appDesc__",
    "default_locale": "en",
    "background": {
        "scripts": [
            "universalwebapp.js",
            "background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "unlimitedStorage",
        "tabs"
    ],
    "content_scripts": [
        {
            "js": [
                "universalwebapp.js",
                "foreground.js"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "all_frames": false
        }
    ],
    "page_action": {
        "default_icon": {
            "19": "Icon-19.png",
            "38": "Icon-38.png"
        },
        "default_title": "Universal WebApp",
        "default_popup": "pageactions.html"
    },
    "icons": {
        "16": "Icon-16.png",
        "48": "Icon-48.png",
        "128": "Icon-128.png"
    },
    "web_accessible_resources": [
        "*"
    ],
    "content_security_policy": "script-src 'self' https://www.google-analytics.com 'unsafe-eval'; object-src 'self'",
    "manifest_version": 2
}
